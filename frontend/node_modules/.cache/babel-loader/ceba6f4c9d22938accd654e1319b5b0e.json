on","trendChart","trend","price_trend","axisPointer","grid","right","bottom","containLabel","xAxis","item","city","axisLabel","yAxis","itemStyle","graphic","LinearGradient","offset","areaChart","area","radius","avoidLabelOverlap","borderRadius","borderColor","borderWidth","fontSize","fontWeight","slice","roomChart","room","room_type_stats","legend","room_type","rotate","position","count","yAxisIndex","orientationChart","orientation","orientation_stats","params","colorList","dataIndex","length","priceRangeChart","priceRange","price_range_distribution","range","wordcloudChart","wordcloud","tags_wordcloud","gridSize","sizeRange","rotationRange","shape","width","height","Math","round","random","join","Object","values","forEach","chart","dispose"],"sources":["src/views/BigScreen.vue"],"sourcesContent":["<template>\n  <div class=\"big-screen-container\">\n    <!-- È°∂ÈÉ®Ê†áÈ¢òÊ†è -->\n    <div class=\"header\">\n      <h1 class=\"title\">üè† Êàø‰ª∑ÂàÜÊûêÂ§ßÂ±è</h1>\n      <div class=\"time\">{{ currentTime }}</div>\n    </div>\n\n    <!-- ‰∏ªË¶ÅÂÜÖÂÆπÂå∫Âüü -->\n    <div class=\"main-content\">\n      <!-- Á¨¨‰∏ÄË°åÔºöÊÄª‰ΩìÊ¶ÇËßà -->\n      <div class=\"row\">\n        <div class=\"card overview-card\" v-for=\"(stat, index) in overviewStats\" :key=\"index\">\n          <div class=\"card-title\">{{ stat.title }}</div>\n          <div class=\"card-value\" :style=\"{ color: stat.color }\">{{ stat.value }}</div>\n          <div class=\"card-desc\">{{ stat.desc }}</div>\n        </div>\n      </div>\n\n      <!-- Á¨¨‰∫åË°åÔºöÂú∞ÂõæÂíå‰ª∑Ê†ºË∂ãÂäø -->\n      <div class=\"row\">\n        <div class=\"card map-card\">\n          <div class=\"card-header\">Âπø‰∏úÁúÅÊàøÊ∫êÂú∞ÁêÜÂàÜÂ∏É</div>\n          <div class=\"card-body\">\n            <div ref=\"mapChart\" class=\"chart-container\"></div>\n          </div>\n        </div>\n        <div class=\"card trend-card\">\n          <div class=\"card-header\">ÂüéÂ∏ÇÂùá‰ª∑ÊéíË°å</div>\n          <div class=\"card-body\">\n            <div ref=\"trendChart\" class=\"chart-container\"></div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Á¨¨‰∏âË°åÔºöÂå∫ÂüüÂàÜÂ∏ÉÂíåÊà∑ÂûãÁªüËÆ° -->\n      <div class=\"row\">\n        <div class=\"card area-card\">\n          <div class=\"card-header\">Âå∫ÂüüÊàøÊ∫êÂàÜÂ∏É</div>\n          <div class=\"card-body\">\n            <div ref=\"areaChart\" class=\"chart-container\"></div>\n          </div>\n        </div>\n        <div class=\"card room-card\">\n          <div class=\"card-header\">Êà∑ÂûãÁªüËÆ°</div>\n          <div class=\"card-body\">\n            <div ref=\"roomChart\" class=\"chart-container\"></div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Á¨¨ÂõõË°åÔºöÊúùÂêëÁªüËÆ°Âíå‰ª∑Ê†ºÂå∫Èó¥ -->\n      <div class=\"row\">\n        <div class=\"card orientation-card\">\n          <div class=\"card-header\">ÊàøÊ∫êÊúùÂêëÂàÜÂ∏É</div>\n          <div class=\"card-body\">\n            <div ref=\"orientationChart\" class=\"chart-container\"></div>\n          </div>\n        </div>\n        <div class=\"card price-range-card\">\n          <div class=\"card-header\">‰ª∑Ê†ºÂå∫Èó¥ÂàÜÂ∏É</div>\n          <div class=\"card-body\">\n            <div ref=\"priceRangeChart\" class=\"chart-container\"></div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Á¨¨‰∫îË°åÔºöËØç‰∫ëÂõæÂíåÊï∞ÊçÆË°®Ê†º -->\n      <div class=\"row\">\n        <div class=\"card wordcloud-card\">\n          <div class=\"card-header\">ÊàøÊ∫êÊ†áÁ≠æËØç‰∫ë</div>\n          <div class=\"card-body\">\n            <div ref=\"wordcloudChart\" class=\"chart-container\"></div>\n          </div>\n        </div>\n        <div class=\"card table-card\">\n          <div class=\"card-header\">ËØ¶ÁªÜÊï∞ÊçÆË°®Ê†º</div>\n          <div class=\"card-body\">\n            <el-table :data=\"tableData\" height=\"300\" stripe>\n              <el-table-column prop=\"city\" label=\"ÂüéÂ∏Ç\" width=\"120\"></el-table-column>\n              <el-table-column prop=\"avg_price\" label=\"Âùá‰ª∑(ÂÖÉ/Êúà)\" width=\"150\">\n                <template slot-scope=\"scope\">\n                  <span class=\"price-text\">¬•{{ scope.row.avg_price }}</span>\n                </template>\n              </el-table-column>\n              <el-table-column prop=\"house_count\" label=\"ÊàøÊ∫êÊï∞Èáè\" width=\"120\"></el-table-column>\n              <el-table-column prop=\"districts\" label=\"ÁÉ≠Èó®Âå∫Âüü\">\n                <template slot-scope=\"scope\">\n                  <el-tag v-for=\"district in scope.row.districts.slice(0, 3)\" \n                          :key=\"district.name\" size=\"mini\" type=\"info\">\n                    {{ district.name }}\n                  </el-tag>\n                </template>\n              </el-table-column>\n            </el-table>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- Âä†ËΩΩÈÅÆÁΩ© -->\n    <div v-if=\"loading\" class=\"loading-overlay\">\n      <div class=\"loading-spinner\"></div>\n      <div class=\"loading-text\">Êï∞ÊçÆÂä†ËΩΩ‰∏≠...</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as echarts from 'echarts'\nimport 'echarts-wordcloud'\nimport guangdongJson from '@/assets/Guangdong.json'\n\nexport default {\n  name: 'BigScreen',\n  data() {\n    return {\n      loading: true,\n      currentTime: '',\n      dashboardData: {},\n      charts: {},\n      timer: null\n    }\n  },\n  computed: {\n    overviewStats() {\n      const data = this.dashboardData.overview || {}\n      return [\n        {\n          title: 'ÊàøÊ∫êÊÄªÊï∞',\n          value: data.total_houses ? data.total_houses.toLocaleString() : '0',\n          desc: 'Â•ó',\n          color: '#409EFF'\n        },\n        {\n          title: 'Âπ≥Âùá‰ª∑Ê†º',\n          value: data.avg_price ? `¬•${data.avg_price.toLocaleString()}` : '¬•0',\n          desc: 'ÂÖÉ/Êúà',\n          color: '#67C23A'\n        },\n        {\n          title: 'ÊúÄÈ´ò‰ª∑Ê†º',\n          value: data.max_price ? `¬•${data.max_price.toLocaleString()}` : '¬•0',\n          desc: 'ÂÖÉ/Êúà',\n          color: '#E6A23C'\n        },\n        {\n          title: 'ÊúÄ‰Ωé‰ª∑Ê†º',\n          value: data.min_price ? `¬•${data.min_price.toLocaleString()}` : '¬•0',\n          desc: 'ÂÖÉ/Êúà',\n          color: '#F56C6C'\n        }\n      ]\n    },\n    tableData() {\n      const geoData = this.dashboardData.geographical_data || {}\n      return geoData.cities || []\n    }\n  },\n  mounted() {\n    this.updateTime()\n    this.timer = setInterval(this.updateTime, 1000)\n    this.loadData()\n  },\n  beforeDestroy() {\n    if (this.timer) {\n      clearInterval(this.timer)\n    }\n    this.disposeCharts()\n  },\n  methods: {\n    updateTime() {\n      const now = new Date()\n      this.currentTime = now.toLocaleString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit'\n      })\n    },\n    async loadData() {\n      try {\n        this.loading = true\n        const response = await this.$http.get('/api/dashboard/data/')\n        if (response.data.success) {\n          this.dashboardData = response.data.data\n          this.$nextTick(() => {\n            this.initCharts()\n          })\n        } else {\n          this.$message.error(response.data.message || 'Êï∞ÊçÆÂä†ËΩΩÂ§±Ë¥•')\n        }\n      } catch (error) {\n        console.error('Âä†ËΩΩÊï∞ÊçÆÂ§±Ë¥•:', error)\n        this.$message.error('Êï∞ÊçÆÂä†ËΩΩÂ§±Ë¥•')\n      } finally {\n        this.loading = false\n      }\n    },\n    initCharts() {\n      this.initMapChart()\n      this.initTrendChart()\n      this.initAreaChart()\n      this.initRoomChart()\n      this.initOrientationChart()\n      this.initPriceRangeChart()\n      this.initWordCloudChart()\n    },\n    initMapChart() {\n      const chartDom = this.$refs.mapChart\n      if (!chartDom) return\n      \n      this.charts.map = echarts.init(chartDom)\n      \n      // Ê≥®ÂÜåÂπø‰∏úÂú∞Âõæ\n      echarts.registerMap('guangdong', guangdongJson)\n      \n      const option = {\n        title: {\n          text: 'Âπø‰∏úÁúÅÊàøÊ∫êÂàÜÂ∏É',\n          left: 'center',\n          textStyle: {\n            color: '#fff'\n          }\n        },\n        tooltip: {\n          trigger: 'item',\n          formatter: '{b}<br/>ÊàøÊ∫êÊï∞Èáè: {c}'\n        },\n        visualMap: {\n          min: 0,\n          max: 5000,\n          left: 'left',\n          top: 'bottom',\n          text: ['È´ò', '‰Ωé'],\n          calculable: true,\n          inRange: {\n            color: ['#50a3ba', '#eac736', '#d94e5d']\n          },\n          textStyle: {\n            color: '#fff'\n          }\n        },\n        series: [\n          {\n            name: 'ÊàøÊ∫êÊï∞Èáè',\n            type: 'map',\n            map: 'guangdong',\n            roam: true,\n            emphasis: {\n              label: {\n                show: true\n              }\n            },\n            data: this.dashboardData.area_distribution || []\n          }\n        ]\n      }\n      \n      this.charts.map.setOption(option)\n    },\n    initTrendChart() {\n      const chartDom = this.$refs.trendChart\n      if (!chartDom) return\n      \n      this.charts.trend = echarts.init(chartDom)\n      \n      const data = this.dashboardData.price_trend || []\n      const option = {\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: {\n            type: 'shadow'\n          }\n        },\n        grid: {\n          left: '3%',\n          right: '4%',\n          bottom: '3%',\n          containLabel: true\n        },\n        xAxis: {\n          type: 'category',\n          data: data.map(item => item.city),\n          axisLabel: {\n            color: '#fff'\n          }\n        },\n        yAxis: {\n          type: 'value',\n          axisLabel: {\n            color: '#fff',\n            formatter: '¬•{value}'\n          }\n        },\n        series: [\n          {\n            name: 'Âùá‰ª∑',\n            type: 'bar',\n            data: data.map(item => item.avg_price),\n            itemStyle: {\n              color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [\n                { offset: 0, color: '#83bff6' },\n                { offset: 0.5, color: '#188df0' },\n                { offset: 1, color: '#188df0' }\n              ])\n            }\n          }\n        ]\n      }\n      \n      this.charts.trend.setOption(option)\n    },\n    initAreaChart() {\n      const chartDom = this.$refs.areaChart\n      if (!chartDom) return\n      \n      this.charts.area = echarts.init(chartDom)\n      \n      const data = this.dashboardData.area_distribution || []\n      const option = {\n        tooltip: {\n          trigger: 'item',\n          formatter: '{a} <br/>{b}: {c} ({d}%)'\n        },\n        series: [\n          {\n            name: 'Âå∫ÂüüÂàÜÂ∏É',\n            type: 'pie',\n            radius: ['40%', '70%'],\n            avoidLabelOverlap: false,\n            itemStyle: {\n              borderRadius: 10,\n              borderColor: '#fff',\n              borderWidth: 2\n            },\n            label: {\n              show: true,\n              color: '#fff'\n            },\n            emphasis: {\n              label: {\n                show: true,\n                fontSize: '16',\n                fontWeight: 'bold'\n              }\n            },\n            data: data.slice(0, 8)\n          }\n        ]\n      }\n      \n      this.charts.area.setOption(option)\n    },\n    initRoomChart() {\n      const chartDom = this.$refs.roomChart\n      if (!chartDom) return\n      \n      this.charts.room = echarts.init(chartDom)\n      \n      const data = this.dashboardData.room_type_stats || []\n      const option = {\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: {\n            type: 'shadow'\n          }\n        },\n        legend: {\n          data: ['ÊàøÊ∫êÊï∞Èáè', 'Âπ≥Âùá‰ª∑Ê†º'],\n          textStyle: {\n            color: '#fff'\n          }\n        },\n        xAxis: {\n          type: 'category',\n          data: data.map(item => item.room_type),\n          axisLabel: {\n            color: '#fff',\n            rotate: 45\n          }\n        },\n        yAxis: [\n          {\n            type: 'value',\n            name: 'Êï∞Èáè',\n            position: 'left',\n            axisLabel: {\n              color: '#fff'\n            }\n          },\n          {\n            type: 'value',\n            name: '‰ª∑Ê†º(ÂÖÉ)',\n            position: 'right',\n            axisLabel: {\n              color: '#fff',\n              formatter: '¬•{value}'\n            }\n          }\n        ],\n        series: [\n          {\n            name: 'ÊàøÊ∫êÊï∞Èáè',\n            type: 'bar',\n            data: data.map(item => item.count),\n            itemStyle: {\n              color: '#5470c6'\n            }\n          },\n          {\n            name: 'Âπ≥Âùá‰ª∑Ê†º',\n            type: 'line',\n            yAxisIndex: 1,\n            data: data.map(item => item.avg_price),\n            itemStyle: {\n              color: '#91cc75'\n            }\n          }\n        ]\n      }\n      \n      this.charts.room.setOption(option)\n    },\n    initOrientationChart() {\n      const chartDom = this.$refs.orientationChart\n      if (!chartDom) return\n      \n      this.charts.orientation = echarts.init(chartDom)\n      \n      const data = this.dashboardData.orientation_stats || []\n      const option = {\n        tooltip: {\n          trigger: 'item',\n          formatter: '{a} <br/>{b}: {c} ({d}%)'\n        },\n        series: [\n          {\n            name: 'ÊúùÂêëÂàÜÂ∏É',\n            type: 'pie',\n            radius: '60%',\n            data: data,\n            itemStyle: {\n              color: function(params) {\n                const colorList = ['#c23531','#2f4554', '#61a0a8', '#d48265', '#91c7ae','#749f83', '#ca8622', '#bda29a']\n                return colorList[params.dataIndex % colorList.length]\n              }\n            },\n            label: {\n              color: '#fff'\n            }\n          }\n        ]\n      }\n      \n      this.charts.orientation.setOption(option)\n    },\n    initPriceRangeChart() {\n      const chartDom = this.$refs.priceRangeChart\n      if (!chartDom) return\n      \n      this.charts.priceRange = echarts.init(chartDom)\n      \n      const data = this.dashboardData.price_range_distribution || []\n      const option = {\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: {\n            type: 'shadow'\n          }\n        },\n        grid: {\n          left: '3%',\n          right: '4%',\n          bottom: '3%',\n          containLabel: true\n        },\n        xAxis: {\n          type: 'value',\n          axisLabel: {\n            color: '#fff'\n          }\n        },\n        yAxis: {\n          type: 'category',\n          data: data.map(item => item.range),\n          axisLabel: {\n            color: '#fff'\n          }\n        },\n        series: [\n          {\n            name: 'ÊàøÊ∫êÊï∞Èáè',\n            type: 'bar',\n            data: data.map(item => item.count),\n            itemStyle: {\n              color: new echarts.graphic.LinearGradient(0, 0, 1, 0, [\n                { offset: 0, color: '#fccb05' },\n                { offset: 1, color: '#f58020' }\n              ])\n            }\n          }\n        ]\n      }\n      \n      this.charts.priceRange.setOption(option)\n    },\n    initWordCloudChart() {\n      const chartDom = this.$refs.wordcloudChart\n      if (!chartDom) return\n      \n      this.charts.wordcloud = echarts.init(chartDom)\n      \n      const data = this.dashboardData.tags_wordcloud || []\n      const option = {\n        tooltip: {\n          show: true,\n          formatter: function(params) {\n            return `${params.data.name}: ${params.data.value}`\n          }\n        },\n        series: [{\n          type: 'wordCloud',\n          gridSize: 8,\n          sizeRange: [12, 50],\n          rotationRange: [-90, 90],\n          shape: 'pentagon',\n          width: '100%',\n          height: '100%',\n          textStyle: {\n            color: function() {\n              return 'rgb(' + [\n                Math.round(Math.random() * 160),\n                Math.round(Math.random() * 160),\n                Math.round(Math.random() * 160)\n              ].join(',') + ')'\n            }\n          },\n          emphasis: {\n            textStyle: {\n              color: '#ff0000'\n            }\n          },\n          data: data\n        }]\n      }\n      \n      this.charts.wordcloud.setOption(option)\n    },\n    disposeCharts() {\n      Object.values(this.charts).forEach(chart => {\n        if (chart) {\n          chart.dispose()\n        }\n      })\n      this.charts = {}\n    }\n  }\n}\n</script>\n\n<style scoped>\n.big-screen-container {\n  width: 100vw;\n  height: 100vh;\n  background: linear-gradient(135deg, #0f2027, #203a43, #2c5364);\n  color: #fff;\n  overflow: hidden;\n  position: relative;\n}\n\n.header {\n  height: 80px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 40px;\n  background: rgba(0, 0, 0, 0.3);\n  border-bottom: 1px solid rgba(255, 255, 255, 0.1);\n}\n\n.title {\n  font-size: 32px;\n  font-weight: bold;\n  background: linear-gradient(to right, #4facfe, #00f2fe);\n  -webkit-background-clip: text;\n  -webkit-text-fill-color: transparent;\n  margin: 0;\n}\n\n.time {\n  font-size: 18px;\n  color: #00f2fe;\n}\n\n.main-content {\n  padding: 20px;\n  height: calc(100vh - 80px);\n  overflow-y: auto;\n}\n\n.row {\n  display: flex;\n  gap: 20px;\n  margin-bottom: 20px;\n}\n\n.card {\n  background: rgba(255, 255, 255, 0.05);\n  border: 1px solid rgba(255, 255, 255, 0.1);\n  border-radius: 8px;\n  backdrop-filter: blur(10px);\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.3);\n}\n\n.card-header {\n  padding: 15px 20px;\n  border-bottom: 1px solid rgba(255, 255, 255, 0.1);\n  font-size: 18px;\n  font-weight: bold;\n  color: #00f2fe;\n}\n\n.card-body {\n  padding: 15px;\n  height: 100%;\n}\n\n.chart-container {\n  width: 100%;\n  height: 100%;\n  min-height: 300px;\n}\n\n/* Á¨¨‰∏ÄË°åÔºöÊ¶ÇËßàÂç°Áâá */\n.overview-card {\n  flex: 1;\n  height: 120px;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  text-align: center;\n}\n\n.card-title {\n  font-size: 16px;\n  color: #aaa;\n  margin-bottom: 10px;\n}\n\n.card-value {\n  font-size: 28px;\n  font-weight: bold;\n  margin-bottom: 5px;\n}\n\n.card-desc {\n  font-size: 14px;\n  color: #888;\n}\n\n/* Á¨¨‰∫åË°åÔºöÂú∞ÂõæÂíåË∂ãÂäøÂõæ */\n.map-card, .trend-card {\n  flex: 1;\n  height: 400px;\n}\n\n/* Á¨¨‰∏âË°åÔºöÂå∫ÂüüÂíåÊà∑Âûã */\n.area-card, .room-card {\n  flex: 1;\n  height: 350px;\n}\n\n/* Á¨¨ÂõõË°åÔºöÊúùÂêëÂíå‰ª∑Ê†ºÂå∫Èó¥ */\n.orientation-card, .price-range-card {\n  flex: 1;\n  height: 350px;\n}\n\n/* Á¨¨‰∫îË°åÔºöËØç‰∫ëÂíåË°®Ê†º */\n.wordcloud-card, .table-card {\n  flex: 1;\n  height: 400px;\n}\n\n.price-text {\n  color: #409EFF;\n  font-weight: bold;\n}\n\n/* Âä†ËΩΩÈÅÆÁΩ© */\n.loading-overlay {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background: rgba(0, 0, 0, 0.8);\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  z-index: 1000;\n}\n\n.loading-spinner {\n  width: 50px;\n  height: 50px;\n  border: 3px solid rgba(255, 255, 255, 0.3);\n  border-radius: 50%;\n  border-top-color: #00f2fe;\n  animation: spin 1s ease-in-out infinite;\n}\n\n.loading-text {\n  margin-top: 20px;\n  color: #00f2fe;\n  font-size: 18px;\n}\n\n@keyframes spin {\n  to { transform: rotate(360deg); }\n}\n\n/* ÊªöÂä®Êù°Ê†∑Âºè */\n.main-content::-webkit-scrollbar {\n  width: 8px;\n}\n\n.main-content::-webkit-scrollbar-track {\n  background: rgba(255, 255, 255, 0.1);\n}\n\n.main-content::-webkit-scrollbar-thumb {\n  background: rgba(0, 242, 254, 0.5);\n  border-radius: 4px;\n}\n\n.main