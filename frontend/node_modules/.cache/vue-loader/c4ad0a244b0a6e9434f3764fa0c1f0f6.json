iv class=\"card-body\">\n            <div ref=\"priceRangeChart\" class=\"chart-container\"></div>\n          </div>\n        </div>\n        \n        <div class=\"card wordcloud-card\">\n          <div class=\"card-header\">\n            <h3>ÊàøÊ∫êÁâπÂæÅËØç‰∫ë</h3>\n          </div>\n          <div class=\"card-body\">\n            <img :src=\"'data:image/png;base64,' + wordcloudData\" alt=\"ËØç‰∫ëÂõæ\" class=\"wordcloud-img\" v-if=\"wordcloudData\">\n            <div v-else class=\"loading-placeholder\">Âä†ËΩΩ‰∏≠...</div>\n          </div>\n        </div>\n      </div>\n      \n      <!-- Á¨¨ÂõõË°åÔºöÁÉ≠Èó®Âå∫ÂüüË°®Ê†ºÂíåÁõ∏ÂÖ≥ÊÄßÂàÜÊûê -->\n      <div class=\"row\">\n        <div class=\"card table-card\">\n          <div class=\"card-header\">\n            <h3>ÁÉ≠Èó®È´ò‰ª∑Âå∫ÂüüTOP10</h3>\n          </div>\n          <div class=\"card-body\">\n            <div class=\"table-container\">\n              <table class=\"data-table\">\n                <thead>\n                  <tr>\n                    <th>ÊéíÂêç</th>\n                    <th>Âå∫Âüü</th>\n                    <th>Âπ≥Âùá‰ª∑Ê†º(ÂÖÉ/„é°)</th>\n                    <th>ÊàøÊ∫êÊï∞Èáè</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"(area, index) in topAreas\" :key=\"index\">\n                    <td>{{ index + 1 }}</td>\n                    <td>{{ area.district }}</td>\n                    <td>{{ area.avg_price.toLocaleString() }}</td>\n                    <td>{{ area.count }}</td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n        \n        <div class=\"card correlation-card\">\n          <div class=\"card-header\">\n            <h3>Êï∞ÊçÆÂàÜÊûêÊ¶ÇËßà</h3>\n          </div>\n          <div class=\"card-body\">\n            <div class=\"stats-grid\">\n              <div class=\"stat-item\" v-if=\"correlationData.statistics\">\n                <div class=\"stat-value\">{{ correlationData.statistics.min_price?.toLocaleString() || '0' }}</div>\n                <div class=\"stat-label\">ÊúÄ‰Ωé‰ª∑Ê†º(ÂÖÉ/„é°)</div>\n              </div>\n              <div class=\"stat-item\" v-if=\"correlationData.statistics\">\n                <div class=\"stat-value\">{{ correlationData.statistics.max_price?.toLocaleString() || '0' }}</div>\n                <div class=\"stat-label\">ÊúÄÈ´ò‰ª∑Ê†º(ÂÖÉ/„é°)</div>\n              </div>\n              <div class=\"stat-item\" v-if=\"correlationData.statistics\">\n                <div class=\"stat-value\">{{ correlationData.statistics.avg_price?.toLocaleString() || '0' }}</div>\n                <div class=\"stat-label\">Âπ≥Âùá‰ª∑Ê†º(ÂÖÉ/„é°)</div>\n              </div>\n              <div class=\"stat-item\" v-if=\"correlationData.statistics\">\n                <div class=\"stat-value\">{{ correlationData.statistics.std_price?.toLocaleString() || '0' }}</div>\n                <div class=\"stat-label\">‰ª∑Ê†ºÊ†áÂáÜÂ∑Æ</div>\n              </div>\n              <div class=\"stat-item\">\n                <div class=\"stat-value\">{{ correlationData.correlation || '0.000' }}</div>\n                <div class=\"stat-label\">Èù¢ÁßØ-‰ª∑Ê†ºÁõ∏ÂÖ≥Á≥ªÊï∞</div>\n              </div>\n              <div class=\"stat-item\">\n                <div class=\"stat-value\">{{ totalHouses }}</div>\n                <div class=\"stat-label\">ÊÄªÊàøÊ∫êÊï∞</div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as echarts from 'echarts'\nimport 'echarts/map/js/china.js'\nimport guangdongJson from '@/../public/Guangdong.json'\nimport { dashboardAPI } from '@/utils/api'\n\nexport default {\n  name: 'Dashboard',\n  data() {\n    return {\n      currentTime: '',\n      mapChart: null,\n      trendChart: null,\n      districtChart: null,\n      typeChart: null,\n      priceRangeChart: null,\n      mapData: [],\n      trendData: {},\n      districtData: {},\n      typeData: {},\n      priceRangeData: {},\n      wordcloudData: '',\n      topAreas: [],\n      correlationData: {},\n      totalHouses: 0\n    }\n  },\n  mounted() {\n    this.initCharts()\n    this.updateTime()\n    this.loadData()\n    \n    // ÊØèÁßíÊõ¥Êñ∞Êó∂Èó¥\n    setInterval(this.updateTime, 1000)\n  },\n  beforeDestroy() {\n    // ÈîÄÊØÅÂõæË°®ÂÆû‰æã\n    if (this.mapChart) this.mapChart.dispose()\n    if (this.trendChart) this.trendChart.dispose()\n    if (this.districtChart) this.districtChart.dispose()\n    if (this.typeChart) this.typeChart.dispose()\n    if (this.priceRangeChart) this.priceRangeChart.dispose()\n  },\n  methods: {\n    updateTime() {\n      const now = new Date()\n      this.currentTime = now.toLocaleString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit',\n        second: '2-digit'\n      })\n    },\n    \n    async loadData() {\n      try {\n        // Âπ∂Ë°åÂä†ËΩΩÊâÄÊúâÊï∞ÊçÆ\n        const [\n          dashboardResponse,\n          mapResponse,\n          trendResponse,\n          districtResponse,\n          typeResponse,\n          priceRangeResponse,\n          wordcloudResponse,\n          topAreasResponse,\n          correlationResponse\n        ] = await Promise.all([\n          dashboardAPI.getDashboardData(),\n          dashboardAPI.getMapData(),\n          dashboardAPI.getPriceTrend(),\n          dashboardAPI.getDistrictDistribution(),\n          dashboardAPI.getTypeDistribution(),\n          dashboardAPI.getPriceRangeDistribution(),\n          dashboardAPI.getWordcloud(),\n          dashboardAPI.getTopAreas(),\n          dashboardAPI.getCorrelationAnalysis()\n        ])\n        \n        if (dashboardResponse.success) {\n          const data = dashboardResponse.data\n          this.mapData = data.map_data || []\n          this.trendData = data.price_trend || {}\n          this.districtData = data.district_dist || {}\n          this.typeData = data.type_dist || {}\n          this.priceRangeData = data.price_range_dist || {}\n          this.wordcloudData = data.wordcloud || ''\n          this.topAreas = data.top_areas || []\n          this.correlationData = data.correlation || {}\n          \n          // ËÆ°ÁÆóÊÄªÊàøÊ∫êÊï∞\n          this.totalHouses = this.mapData.reduce((sum, item) => sum + (item.value[1] || 0), 0)\n        }\n        \n        this.updateCharts()\n      } catch (error) {\n        console.error('Êï∞ÊçÆÂä†ËΩΩÂ§±Ë¥•:', error)\n        this.$message.error('Êï∞ÊçÆÂä†ËΩΩÂ§±Ë¥•')\n      }\n    },\n    \n    refreshData() {\n      this.loadData()\n      this.$message.success('Êï∞ÊçÆÂà∑Êñ∞ÊàêÂäü')\n    },\n    \n    initCharts() {\n      // Ê≥®ÂÜåÂπø‰∏úÂú∞Âõæ\n      echarts.registerMap('guangdong', guangdongJson)\n      \n      // ÂàùÂßãÂåñÂêÑ‰∏™ÂõæË°®\n      this.mapChart = echarts.init(this.$refs.mapChart)\n      this.trendChart = echarts.init(this.$refs.trendChart)\n      this.districtChart = echarts.init(this.$refs.districtChart)\n      this.typeChart = echarts.init(this.$refs.typeChart)\n      this.priceRangeChart = echarts.init(this.$refs.priceRangeChart)\n      \n      // ÁõëÂê¨Á™óÂè£Â§ßÂ∞èÂèòÂåñ\n      window.addEventListener('resize', this.handleResize)\n    },\n    \n    handleResize() {\n      this.mapChart.resize()\n      this.trendChart.resize()\n      this.districtChart.resize()\n      this.typeChart.resize()\n      this.priceRangeChart.resize()\n    },\n    \n    updateCharts() {\n      this.updateMapChart()\n      this.updateTrendChart()\n      this.updateDistrictChart()\n      this.updateTypeChart()\n      this.updatePriceRangeChart()\n    },\n    \n    updateMapChart() {\n      const option = {\n        title: {\n          text: 'Âπø‰∏úÁúÅÊàø‰ª∑ÂàÜÂ∏É',\n          left: 'center',\n          textStyle: {\n            color: '#fff'\n          }\n        },\n        tooltip: {\n          trigger: 'item',\n          formatter: function(params) {\n            if (params.value) {\n              return `${params.name}<br/>Âùá‰ª∑: ${params.value[0]?.toLocaleString()}ÂÖÉ/„é°<br/>ÊàøÊ∫êÊï∞: ${params.value[1]}Â•ó<br/>Âπ≥ÂùáÈù¢ÁßØ: ${params.value[2]}„é°`\n            }\n            return params.name\n          }\n        },\n        visualMap: {\n          min: 0,\n          max: 100000,\n          calculable: true,\n          inRange: {\n            color: ['#50a3ba', '#eac736', '#d94e5d']\n          },\n          textStyle: {\n            color: '#fff'\n          }\n        },\n        series: [{\n          name: 'Êàø‰ª∑',\n          type: 'map',\n          map: 'guangdong',\n          roam: true,\n          zoom: 1.2,\n          label: {\n            show: true,\n            color: '#fff'\n          },\n          data: this.mapData\n        }]\n      }\n      this.mapChart.setOption(option)\n    },\n    \n    updateTrendChart() {\n      const option = {\n        title: {\n          text: 'ÂéÜÂπ¥Êàø‰ª∑Ëµ∞Âäø',\n          left: 'center',\n          textStyle: {\n            color: '#fff'\n          }\n        },\n        tooltip: {\n          trigger: 'axis',\n          formatter: '{b}: {c}ÂÖÉ/„é°'\n        },\n        xAxis: {\n          type: 'category',\n          data: this.trendData.years || [],\n          axisLine: { lineStyle: { color: '#fff' } },\n          axisLabel: { color: '#fff' }\n        },\n        yAxis: {\n          type: 'value',\n          axisLine: { lineStyle: { color: '#fff' } },\n          axisLabel: { \n            color: '#fff',\n            formatter: '{value}'\n          }\n        },\n        series: [{\n          data: this.trendData.prices || [],\n          type: 'line',\n          smooth: true,\n          lineStyle: { color: '#50a3ba' },\n          itemStyle: { color: '#50a3ba' },\n          areaStyle: { color: 'rgba(80, 163, 186, 0.3)' }\n        }]\n      }\n      this.trendChart.setOption(option)\n    },\n    \n    updateDistrictChart() {\n      const option = {\n        title: {\n          text: 'ÁÉ≠Èó®Âå∫ÂüüÊàøÊ∫êÊï∞',\n          left: 'center',\n          textStyle: {\n            color: '#fff'\n          }\n        },\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: { type: 'shadow' }\n        },\n        xAxis: {\n          type: 'category',\n          data: this.districtData.districts || [],\n          axisLine: { lineStyle: { color: '#fff' } },\n          axisLabel: { \n            color: '#fff',\n            rotate: 45\n          }\n        },\n        yAxis: {\n          type: 'value',\n          axisLine: { lineStyle: { color: '#fff' } },\n          axisLabel: { color: '#fff' }\n        },\n        series: [{\n          data: this.districtData.counts || [],\n          type: 'bar',\n          itemStyle: {\n            color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{\n              offset: 0,\n              color: '#50a3ba'\n            }, {\n              offset: 1,\n              color: '#eac736'\n            }])\n          }\n        }]\n      }\n      this.districtChart.setOption(option)\n    },\n    \n    updateTypeChart() {\n      const option = {\n        title: {\n          text: 'ÊàøÂ±ãÁ±ªÂûãÂç†ÊØî',\n          left: 'center',\n          textStyle: {\n            color: '#fff'\n          }\n        },\n        tooltip: {\n          trigger: 'item',\n          formatter: '{a} <br/>{b}: {c} ({d}%)'\n        },\n        legend: {\n          bottom: '5%',\n          left: 'center',\n          textStyle: { color: '#fff' }\n        },\n        series: [{\n          name: 'ÊàøÂ±ãÁ±ªÂûã',\n          type: 'pie',\n          radius: ['40%', '70%'],\n          center: ['50%', '50%'],\n          data: (this.typeData.types || []).map((type, index) => ({\n            name: type,\n            value: this.typeData.counts[index] || 0\n          })),\n          itemStyle: {\n            borderRadius: 10,\n            borderColor: '#fff',\n            borderWidth: 2\n          },\n          label: {\n            color: '#fff'\n          }\n        }]\n      }\n      this.typeChart.setOption(option)\n    },\n    \n    updatePriceRangeChart() {\n      const option = {\n        title: {\n          text: '‰ª∑Ê†ºÂå∫Èó¥ÂàÜÂ∏É',\n          left: 'center',\n          textStyle: {\n            color: '#fff'\n          }\n        },\n        tooltip: {\n          trigger: 'axis',\n          axisPointer: { type: 'shadow' }\n        },\n        xAxis: {\n          type: 'category',\n          data: this.priceRangeData.labels || [],\n          axisLine: { lineStyle: { color: '#fff' } },\n          axisLabel: { color: '#fff' }\n        },\n        yAxis: {\n          type: 'value',\n          axisLine: { lineStyle: { color: '#fff' } },\n          axisLabel: { color: '#fff' }\n        },\n        series: [{\n          data: this.priceRangeData.counts || [],\n          type: 'bar',\n          itemStyle: {\n            color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{\n              offset: 0,\n              color: '#eac736'\n            }, {\n              offset: 1,\n              color: '#d94e5d'\n            }])\n          }\n        }]\n      }\n      this.priceRangeChart.setOption(option)\n    }\n  }\n}\n</script>\n\n<style scoped>\n.dashboard-container {\n  background: #0a1929;\n  color: #fff;\n  min-height: 100vh;\n  padding: 20px;\n}\n\n.header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 20px;\n  padding: 15px 20px;\n  background: rgba(255, 255, 255, 0.1);\n  border-radius: 10px;\n  backdrop-filter: blur(10px);\n}\n\n.header h1 {\n  margin: 0;\n  font-size: 28px;\n  color: #50a3ba;\n}\n\n.header-info {\n  display: flex;\n  align-items: center;\n  gap: 20px;\n}\n\n.time {\n  font-size: 16px;\n  color: #eac736;\n}\n\n.refresh-btn {\n  cursor: pointer;\n  padding: 8px 15px;\n  background: rgba(80, 163, 186, 0.3);\n  border-radius: 5px;\n  transition: background 0.3s;\n}\n\n.refresh-btn:hover {\n  background: rgba(80, 163, 186, 0.5);\n}\n\n.main-content {\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n}\n\n.row {\n  display: flex;\n  gap: 20px;\n}\n\n.card {\n  background: rgba(255, 255, 255, 0.05);\n  border-radius: 10px;\n  backdrop-filter: blur(10px);\n  border: 1px solid rgba(255, 255, 255, 0.1);\n  overflow: hidden;\n  flex: 1;\n}\n\n.card-header {\n  padding: 15px 20px;\n  border-bottom: 1px solid rgba(255, 255, 255, 0.1);\n}\n\n.card-header h3 {\n  margin: 0;\n  color: #50a3ba;\n  font-size: 18px;\n}\n\n.card-body {\n  padding: 20px;\n  height: 400px;\n}\n\n.chart-container {\n  width: 100%;\n  height: 100%;\n}\n\n/* ÁâπÊÆäÂç°ÁâáÊ†∑Âºè */\n.map-card {\n  flex: 1.5;\n}\n\n.table-card, .correlation-card {\n  flex: 1;\n}\n\n.table-container {\n  height: 100%;\n  overflow-y: auto;\n}\n\n.data-table {\n  width: 100%;\n  border-collapse: collapse;\n  color: #fff;\n}\n\n.data-table th,\n.data-table td {\n  padding: 12px;\n  text-align: left;\n  border-bottom: 1px solid rgba(255, 255, 255, 0.1);\n}\n\n.data-table th {\n  background: rgba(80, 163, 186, 0.2);\n  font-weight: 600;\n}\n\n.data-table tr:hover {\n  background: rgba(255, 255, 255, 0.05);\n}\n\n.wordcloud-img {\n  max-width: 100%;\n  max-height: 100%;\n  object-fit: contain;\n}\n\n.loading-placeholder {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  height: 100%;\n  color: #eac736;\n  font-size: 18px;\n}\n\n.stats-grid {\n  display: grid;\n  grid-template-columns: repeat(2, 1fr);\n  gap: 20px;\n  height: 100%;\n}\n\n.stat-item {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  padding: 20px;\n  background: rgba(80, 163, 186, 0.1);\n  border-radius: 10px;\n  text-align: center;\n}\n\n.stat-value {\n  font-size: 24px;\n  font-weight: bold;\n  color: #50a3ba;\n  margin-bottom: 5px;\n}\n\n.stat-label {\n  font-size: 14px;\n  color: #eac736;\n}\n\n/* ÊªöÂä®Êù°Ê†∑Âºè */\n.table-container::-webkit-scrollbar {\n  width: 6px;\n}\n\n.table-container::-webkit-scrollbar-track {\n  background: rgba(255, 255, 255, 0.1);\n  border-radius: 3px;\n}\n\n.table-container::-webkit-scrollbar-thumb {\n  background: rgba(80, 163, 186, 0.5);\n  border-radius: 3px;\n}\n\n.table-container::-webkit-scrollbar-thumb:hover {\n  background: rgba(80, 163, 186, 0.7);\n}\n</style>"]}]}