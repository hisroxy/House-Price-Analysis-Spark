3:\n          // 拒绝访问\n          console.error('拒绝访问')\n          break\n        case 500:\n          // 服务器错误\n          console.error('服务器错误')\n          break\n        default:\n          console.error(`连接错误${error.response.status}`)\n      }\n    } else {\n      console.error('网络错误')\n    }\n    return Promise.reject(error)\n  }\n)\n\n// 用户相关API\nexport const userAPI = {\n  // 注册\n  register(userData) {\n    return api.post('/user/api/register/', userData)\n  },\n  \n  // 登录\n  login(credentials) {\n    return api.post('/user/api/login/', credentials)\n  },\n  \n  // 登出\n  logout() {\n    return api.post('/user/api/logout/')\n  },\n  \n  // 获取用户信息\n  getUserInfo() {\n    return api.get('/user/api/user-info/')\n  }\n}\n\n// 大屏数据API\nexport const dashboardAPI = {\n  // 获取完整大屏数据\n  getDashboardData() {\n    return api.get('/dashboard/api/dashboard-data/')\n  },\n  \n  // 获取地图数据\n  getMapData() {\n    return api.get('/dashboard/api/map-data/')\n  },\n  \n  // 获取价格趋势数据\n  getPriceTrend() {\n    return api.get('/dashboard/api/price-trend/')\n  },\n  \n  // 获取区域分布数据\n  getDistrictDistribution() {\n    return api.get('/dashboard/api/district-dist/')\n  },\n  \n  // 获取房屋类型分布数据\n  getTypeDistribution() {\n    return api.get('/dashboard/api/type-dist/')\n  },\n  \n  // 获取价格区间分布数据\n  getPriceRangeDistribution() {\n    return api.get('/dashboard/api/price-range/')\n  },\n  \n  // 获取词云图数据\n  getWordcloud() {\n    return api.get('/dashboard/api/wordcloud/')\n  },\n  \n  // 获取热门区域数据\n  getTopAreas() {\n    return api.get('/dashboard/api/top-areas/')\n  },\n  \n  // 获取相关性分析数据\n  getCorrelationAnalysis() {\n    return api.get('/dashboard/api/correlation/')\n  }\n}\n\nexport default api"],"mappings":"AAAA,OAAOA,KAAK,MAAM,OAAO;;AAEzB;AACA,MAAMC,GAAG,GAAGD,KAAK,CAACE,MAAM,CAAC;EACvBC,OAAO,EAAE,uBAAuB;EAChCC,OAAO,EAAE,KAAK;EACdC,eAAe,EAAE,IAAI,CAAC;AACxB,CAAC,CAAC;;AAEF;AACAJ,GAAG,CAACK,YAAY,CAACC,OAAO,CAACC,GAAG,CAC1BC,MAAM,IAAI;EACR;EACA,OAAOA,MAAM;AACf,CAAC,EACDC,KAAK,IAAI;EACP,OA